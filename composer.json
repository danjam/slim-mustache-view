{
  "name": "danjam/slim-mustache-view",
  "type": "library",
  "description": "Simple Slim 3 framework view renderer for mustache templates using Mustache.php",
  "keywords": ["slim","framework","view","template","mustache"],
  "homepage": "https://github.com/danjam/slim-mustache-view",
  "license": "MIT",
  "authors": [
    {
      "name": "Danny James",
      "email": "hello@dannyjames.net",
      "homepage": "http://dannyjames.net"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "mustache/mustache": "^2.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "slim/slim": "^3.0.0",
    "phpunit/phpunit": "^4.6.0",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Slim\\Views\\": "src"
    }
  }
}
